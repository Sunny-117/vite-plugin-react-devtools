{
  "name": "vite-plugin-react-devtools",
  "type": "module",
  "version": "0.0.0-alpha.1",
  "description": "The next iteration of React DevTools",
  "author": "Sunny-117",
  "license": "MIT",
  "homepage": "https://github.com/Sunny-117/vite-plugin-react-devtools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunny-117/vite-plugin-react-devtools.git"
  },
  "bugs": "https://github.com/Sunny-117/vite-plugin-react-devtools/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/ws": "^8.18.1",
    "react-devtools-core": "^6.1.5",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.0-beta.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^20.14.10",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "esno": "^4.7.0",
    "rimraf": "^5.0.8",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  }
}
